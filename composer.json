{
  "name": "aperturepro/crm",
  "description": "A privacy-focused CRM for photographers.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Ian R. Gordon",
      "email": "hello@iangordon.app",
      "homepage": "https://iangordon.app"
    }
  ],
  "require": {
    "php": ">=7.4",
    "stripe/stripe-php": "^10.0",
    "google/apiclient": "^2.15.0"
  },
  "autoload": {
    "psr-4": {
      "AperturePro\\": "includes/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-update-cmd": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Calendar"
    ]
  }
}
